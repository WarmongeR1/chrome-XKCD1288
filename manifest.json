{
  "manifest_version": 2,
  "name": "XKCD 1288",
  "version": "1.0",
  "description": "Повышаем на 300% привлекательность текста - заменой унылых слов на веселые",

  "author": "Sapronov Alexander sapronov.alexander92<@>gmail.com",

  "icons": {
      "128": "images/icon_128.png",
      "48": "images/icon_48.png",
      "32": "images/icon_32.png",
      "16": "images/icon_16.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["scripts/jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
      "default_icon": "images/icon_128.png"
  }
}